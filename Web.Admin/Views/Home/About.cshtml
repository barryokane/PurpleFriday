@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<p>Use this area to provide additional information.</p>



<hr />
<input type="button" value="Click me" id="TestPost" />



@section Scripts {


<script>$("#TestPost").click(function () {
        $.ajax({
            url: "/api/map",
            type: "POST",
            data: JSON.stringify(
                {
                    "TweetId": S4() + "-" + S4(),
                    "CreatedDate": "2019-06-08T17:06:29+01:00",
                    "img": "https://pbs.twimg.com/media/D8e7vBgWsAI0uns.jpg",
                    "TweetUrl": "https://twitter.com/LGBTYS/status/1135926878202748930",
                    "LocationConfidence": "High",
                    "text": "Text TEST",
                    "twitterhandle": "LGBTYS",
                    "geo": [57.83, -5.3]

                }),
            contentType: "application/json"
        })
    });

    function S4() {
        return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
    }</script>
}